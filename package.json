{
  "name": "longevity-map",
  "version": "0.0.1",
  "description": "Longevity Map Website",
  "main": "src/server/index.ts",
  "scripts": {
    "build": "webpack --mode production",
    "start": "yarn run build && ts-node src/server/index.ts",
    "dev-start": "yarn run client && ts-node src/server/index.ts",
    "client": "webpack serve --mode development --devtool inline-source-map --hot",
    "server": "nodemon --watch './**/*.ts' --exec 'ts-node' src/server/index.ts",
    "dev": "concurrently \"yarn run server\" \"yarn run client\"",
    "test": "jest"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "ag-grid-community": "^28.1.1",
    "ag-grid-react": "^28.1.1",
    "bluebird": "^3.7.2",
    "cache-manager": "^4.1.0",
    "class-transformer": "^0.5.1",
    "express": "^4.18.1",
    "immutable": "^4.1.0",
    "install": "^0.13.0",
    "jest": "^29.0.3",
    "jsonata": "^1.8.6",
    "mathjs": "^11.1.0",
    "plotly.js": "^2.14.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropdown-select": "^4.9.0",
    "react-plotly.js": "^2.6.0",
    "react-promise-tracker": "^2.1.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "semantic-ui-react": "2.1.3",
    "yarn": "^1.22.19"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@types/cache-manager": "^4.0.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.19.0",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.19.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.0.0",
    "@types/node": "^18.7.16",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/plotly.js": "^2.12.2",
    "@types/react-plotly.js": "^2.5.1",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
